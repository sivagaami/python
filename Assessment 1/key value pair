S = {}
while True:
 input=input("Do you want to Add/Modify/Exit:")
 if input == "Add":
        key = input("Enter the key: ")
        value = input("Enter the value: ")
        S[key]=value
        print("Dict values are",S)
 elif input == "Modify":
     key = input("Enter the key: ")
     value = input("Enter the value: ")
     S[key]=value
     print("Dict values are",S)
 else:
   quit()
